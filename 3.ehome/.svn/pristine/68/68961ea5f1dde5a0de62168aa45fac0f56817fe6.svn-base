/*头部样式*/
header {
    line-height: 49px;
    text-align: center;
    font-size: 17px;
    font-weight: 500;
    color: #000000;
    background-color: #f8f8f8;
    position: relative;
}

header::after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 1px;
    background-color: #f8f8f8;
    -webkit-transform-origin: left bottom;
    transform-origin: left bottom;
}

header span:nth-child(2) {
    margin-left: -30px;
}

/*返回按钮的样式*/
#demo {
    position: absolute;
    display: inline-block;
    left: 30px;
    top: 0;
    bottom: 0;
}

#demo::before {
    content: " ";
    display: inline-block;
    height: 10px;
    width: 10px;
    line-height: 49px;
    border-width: 2px 2px 0 0;
    border-color: #000000;
    border-style: solid;
    position: absolute;
    top: 50%;
    margin-top: -4px;
    right: 2px;
    transform: rotate(-135deg);
}

