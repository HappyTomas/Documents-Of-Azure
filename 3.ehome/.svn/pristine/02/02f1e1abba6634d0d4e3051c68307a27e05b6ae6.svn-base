        <div class="grey_top">
            {{buildingName}}<i class="glyphicon glyphicon-triangle-bottom" v-on:click="changeBuilding()" style="margin-left: 5px; font-size: 12px;"></i>
        </div>
        <!--轮播图片-->
        <div class="index_menu_main" style="margin-top: 0; padding-bottom: 0;" v-if="topAd && topAd.length>0">
