package com.its.modules.app.bean;

import java.util.Date;

import com.its.modules.app.entity.CouponManage;

public class CouponManageBean extends CouponManage {

	private static final long serialVersionUID = 1L;
	// 会员的优惠券ID
	private String memberDiscountId;
	private Date validStart;
	private Date validEnd;
	// 会员的优惠券使用状态
	private String useState;

	public String getMemberDiscountId() {
		return memberDiscountId;
	}

	public void setMemberDiscountId(String memberDiscountId) {
		this.memberDiscountId = memberDiscountId;
	}

	public Date getValidStart() {
		return validStart;
	}

	public void setValidStart(Date validStart) {
		this.validStart = validStart;
	}

	public Date getValidEnd() {
		return validEnd;
	}

	public void setValidEnd(Date validEnd) {
		this.validEnd = validEnd;
	}

	public String getUseState() {
		return useState;
	}

	public void setUseState(String useState) {
		this.useState = useState;
	}
}