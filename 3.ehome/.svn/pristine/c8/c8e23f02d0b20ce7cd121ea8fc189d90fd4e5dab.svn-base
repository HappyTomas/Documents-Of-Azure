package com.its.modules.social.bean;

import com.its.modules.social.entity.SocialComment;

/**
 * @Description：查询结果封装bean
 * @Author：刘浩浩
 * @Date：2017年8月4日
 */
public class SocialCommentBean extends SocialComment {

	private static final long serialVersionUID = 1L;
	
	private String nickName;//昵称
	
	private String photo;//头像
	
	private int isPraise;//是否点赞  1:是 0:否
	
	private int countPraise;//点赞数量

	public int getIsPraise() {
		return isPraise;
	}

	public void setIsPraise(int isPraise) {
		this.isPraise = isPraise;
	}

	public int getCountPraise() {
		return countPraise;
	}

	public void setCountPraise(int countPraise) {
		this.countPraise = countPraise;
	}

	public String getNickName() {
		return nickName;
	}

	public void setNickName(String nickName) {
		this.nickName = nickName;
	}

	public String getPhoto() {
		return photo;
	}

	public void setPhoto(String photo) {
		this.photo = photo;
	}

	public static long getSerialversionuid() {
		return serialVersionUID;
	}
	
	
}