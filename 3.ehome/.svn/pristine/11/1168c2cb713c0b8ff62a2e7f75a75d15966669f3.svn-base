    #{balanceCycle},
    #{businessIntroduce},
    #{businessState},
    #{stopBusinessBeginTime},
    #{stopBusinessEndTime},
    #{serviceModel},
    #{serviceTimeInterval},
    #{serviceCharge},
    #{shortestTime},
    #{distributeModel},
    #{distributeTimeInterval},
    #{shortestArriveTime},
    #{distributeCharge},
